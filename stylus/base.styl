// смс подтверждение новое
.sms_confirm
	padding 25rem
	font-size fzn - 1rem
	// background cbginfo
	text-align center
	p
		font-weight fwl
		line-height 1.5!important
	p.small_text
		margin-bottom 0!important
		a
			font-size fzn - 2rem
	.sms_phone
		display block
		font-size 20rem
	.sms_cancel
		margin-top 5rem
		display inline-block
	.sms_field
		margin 0 0 20rem
		input[type="text"]
			letter-spacing 5px
			font-size 32rem
			font-weight fwl
			font-family Tahoma, 'Trubuchet MS'
			background-color cbgsky
			border-radius 5rem
			border 1rem solid transparent
			width 150rem
			padding 0 0 0 20rem
			height 55rem
			box-shadow none
			transition color .05s, width .2s, height .2s, border-radius .2s, background .2s, border .2s

			&::-webkit-input-placeholder
				color rgba(0,0,0,0.1)
				font-weight fwl
				letter-spacing 5px
			&::-moz-placeholder
				color rgba(0,0,0,0.1)
				font-weight fwl
				letter-spacing 5px
			&:-ms-input-placeholder
				color rgba(0,0,0,0.1)!important
				font-weight fwl
				letter-spacing 5px
			&.error
				animation: shake 1s cubic-bezier(.36,.07,.19,.97) both;
				transform: translate3d(0, 0, 0);
				backface-visibility: hidden;
				perspective 1000rem
				color ctexterror
				background-color #fbebee


				&::-webkit-input-placeholder
					color rgba(255,0,0,0.2)

				&::-moz-placeholder
					color rgba(255,0,0,0.2)

				&:-ms-input-placeholder
					color rgba(255,0,0,0.2)!important
			&.success
				background-color clsub6
				color ctextsuccess
			&.loader
				width 55rem
				height 55rem
				color transparent
				background transparent
				border-radius 50rem
				border-bottom 5rem solid cl2
				border-top 5rem solid cl3
				border-left 5rem solid cl1
				border-right 5rem solid cl4
				animation rotatingSms 1s .2s linear infinite;

	.form_buttons
		margin 25rem 0 20rem

	.captcha
		padding 0
		border none
		text-align center
		.captcha_left
			float none
			display inline-block

.popup_layer .sms_confirm
	margin-right -40rem
	margin-left -40rem
	margin-bottom -35rem


