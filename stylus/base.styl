// -------------- dropdown ---------------
[dropdown]
	display inline-block

.dropdown_menu, .dropdown_block
	display none
	position absolute
	z-index 1000
	top 100%
	left 0
	background #fff
	box-shadow 0 5rem 25rem rgba(0,0,0,0.1)
.dropdown_block
	padding 10rem
.dropdown_menu
