@import 'nib'
@import 'variables.styl'


::-webkit-scrollbar
	height 12rem
	width 12rem
	background transparent
::-webkit-scrollbar-thumb
	border-radius 10rem
	cursor pointer
	width 12rem
	cursor pointer
	border 4rem solid rgba(255, 255, 255, 0)
	background-clip padding-box
	background-color rgba(cl1, 0.1)
	// box-shadow inset -1rem -1rem 0rem rgba(0, 0, 0, 0.03), inset 1rem 1rem 0rem rgba(0, 0, 0, 0.03)
	&:hover
		background-color rgba(cl1, 0.4)
		cursor pointer


*
	margin 0
	padding 0
	vertical-align baseline
	outline none
html
	font-size 1px

@media screen and (min-width:1600px) and (min-height:750px) {
    html {font-size: 1.105px;}
}
@media screen and (min-width:1900px) and (min-height:750px) {
    html {font-size: 1.2px;}
}


html, body
	position relative
	min-height 100%
	height 100%

body
	font-size fzn
	font-family ffn
	font-weight fwn
	color ctext
	line-height 1.3
	-webkit-font-smoothing: antialiased



div, article, aside, footer, header, nav, section, dialog, figure, hgroup, menu
	position relative

a
	color clink
	text-decoration underline
	cursor pointer
	transition color .1s ease
	&:hover
		color cl2
	&:active
		color cl1
		transition none
	&:focus
		outline -webkit-focus-ring-color auto 3rem

	&.link_dashed
		text-decoration none
		color clink
		&>span
			border-bottom 1rem dashed @color
		&:hover
			color cl2
			span
				border-bottom-color cl2
	&.link_disabled
		color ctextdis!important
		cursor default

	&[class*='icon-']
		&:before
			transition 0
		&:empty
			text-decoration none
		@media all and (-ms-high-contrast: none), (-ms-high-contrast: active)
			text-decoration none
	&[class*='icon-close']
		transition transform .15s
		&:before
			transition transform .15s
			margin 0
			min-width 0
		// &:hover
		// 	&:before
		// 		transform rotate(90deg)
		&:active
			&:before
				transform scale(.9)
				transition 0s
	&[class*='icon-favorites-solid']
		&:before
			color ctextwarning!important

	&[class*='icon-favorites']
		&:hover
			&:before
				color ctextwarning!important

	&.c_error
		&:hover
			color ctexterror + 10%

p
	line-height 1.5

ul
	list-style none
	&.circ
		margin 0 0 15rem
		li
			background url(../images/ul_circ.svg) no-repeat 0 7rem
			margin 0 0 10rem 7rem
			padding-left 20rem
	&.point
		margin 0 0 15rem
		li
			margin 0 0 5rem 7rem
			padding-left 15rem
			position relative
			&:before
				content '•'
				position absolute
				top 0rem
				left 0
				font-size fzn

b, strong
	font-weight fwm
i, em
	font-style italic
h1, h2, h3, h4
	font-weight fwm
h1
	font-size 26rem
	font-weight fwl
	margin 0 0 30rem
h2
	font-size 20rem
	margin 0 0 20rem
	font-weight fwn
h3
	font-size 18rem
	margin 0 0 20rem
