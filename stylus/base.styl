.blur
	min-height 100%
	height 100%
	-webkit-filter blur(4rem)

.modal
	position absolute
	left 0
	top 0
	height 100%
	width 100%

.popup_overlay
	position absolute
	left 0
	top 0
	overflow auto
	height 100%
	width 100%
	background rgba(cl1,0.34)
