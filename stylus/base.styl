// загрузка
.loading
	margin 50rem auto
	display block
	text-align center
	div
		display inline-block
		background url(../images/loading.gif) no-repeat center 0
		background-size 24rem 24rem
		padding 40rem 0 0
		min-height 24rem
		min-width 24rem
		max-width 350rem
		font-size fzn - 2rem
		color ctext
		box-sizing border-box
		line-height 1.5
		.title
			font-size fzn
			margin 0 0 10rem
			display block
	&.info
		margin 0 0 15rem
		div
			max-width 1000rem
			color ctext
	&.full
		div
			max-width 100%


.loading_block
	position relative
	&[data-loading-text]
		&:after
			background-position center calc(50% - 12rem)
	&:before
		content ' 'attr(data-loading-text)' '
		position absolute
		top 50%
		left 50%
		text-align center
		z-index 101
		transform translate(-50%, 10rem)
		font-size fzn - 2rem
		color ctext
		text-shadow 0 0 7rem #fff, 0 0 7rem #fff, 0 0 7rem #fff, 0 0 7rem #fff
		width 80%

	&:after
		transition background-color .5s ease
		content ''
		position absolute
		top 0
		right 0
		bottom 0
		left 0
		background-color rgba(#fff,0.6)
		background-image url(../images/loading.gif)
		background-repeat no-repeat
		background-position center
		background-size 24rem 24rem
		z-index 100

.loading_block_clean
	overflow hidden!important
	background-color transparent
	background-image url(../images/loading.gif)
	background-repeat no-repeat
	background-position center
	background-size 24rem 24rem
	max-height 200rem
	& *
		opacity 0


.disable_block
	position relative
	> *
		opacity .4

	&:after
		content ''
		position absolute
		top 0
		right 0
		bottom 0
		left 0
		z-index 10




