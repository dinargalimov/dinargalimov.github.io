// анимированный фон
.back_anim
	position absolute
	top 0
	bottom 0
	right 0
	left 0
	overflow hidden
	z-index 0
	display none
	[class*="cub"]
		position absolute
	.cub1
		height 400rem
		width 16vw
		min-width 240rem
		background url(../images/cub1.png) no-repeat 0 0
		background-size 100% auto
		bottom 40rem
		left 12%
		animation cub1 30s linear 2
	.cub2
		height 400rem
		width 16vw
		margin 240rem
		background url(../images/cub2.png) no-repeat 0 0
		background-size 100% auto
		top -100rem
		right -100rem
		animation cub2 20s linear 3
	.cub3
		height 143rem
		width 143rem
		background url(../images/cub3.png) no-repeat 0 0
		top 150rem
		left 15%
		animation cub3 30s linear 2
	.cub4
		height 131rem
		width 131rem
		background url(../images/cub4.png) no-repeat 0 0
		bottom 10%
		right 21%
