@keyframes cub1 {
    from { transform: rotate(-180deg) translateX(150rem) rotate(360deg); }
    to   { transform: rotate(180deg) translateX(150rem) rotate(0deg); }
}
@keyframes cub2 {
    from { transform: rotate(0deg) translateX(50rem) rotate(0deg); }
    to   { transform: rotate(360deg) translateX(50rem) rotate(-360deg); }
}
@keyframes cub3 {
    from { transform: rotate(0deg) translateX(100rem) rotate(0deg); }
    to   { transform: rotate(-360deg) translateX(100rem) rotate(360deg); }
}
@keyframes cub4 {
    from { transform: rotate(180deg) translateX(200rem) rotate(0deg); }
    to   { transform: rotate(-180deg) translateX(200rem) rotate(360deg); }
}
@keyframes cub5 {
    from { transform: rotate(90deg) translateX(50rem) rotate(0deg); }
    to   { transform: rotate(450deg) translateX(50rem) rotate(-360deg); }
}
@keyframes cub6 {
    from { transform: rotate(180deg) translateX(50rem) rotate(0deg); }
    to   { transform: rotate(-180deg) translateX(50rem) rotate(360deg); }
}
@keyframes cub7 {
    from { transform: rotate(180deg) translateX(50rem) rotate(0deg); }
    to   { transform: rotate(-180deg) translateX(50rem) rotate(360deg); }
}
@keyframes cub8 {
    from { transform: rotate(90deg) translateX(50rem) rotate(0deg); }
    to   { transform: rotate(-270deg) translateX(50rem) rotate(360deg); }
}
@keyframes cub9 {
    from { transform: rotate(-180deg) translateX(50rem) rotate(0deg); }
    to   { transform: rotate(180deg) translateX(50rem) rotate(-360deg); }
}


