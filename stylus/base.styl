// ------------  tooltip -----------------

.tooltip
	position absolute
	z-index 2700
	display block
	visibility visible
	font-weight normal
	opacity 0
	background #35414C
	border-radius 3rem
	// border 1rem solid cbd
	// box-shadow 0rem 2rem 5rem 0rem rgba(0,0,0,0.11)
	color #fff
	margin-right -1000rem
	margin-bottom -1000rem

	&:after
		border solid transparent
		content " "
		height 0
		width 0
		position absolute
		pointer-events none
	&:after
		border-color rgba(255, 255, 255, 0)
		border-width 6rem
	// &:before
	// 	border-color rgba(208, 211, 215, 0)
	// 	border-width 7rem

	&.error
		background rgba(cerror, 1)
		// .tooltip-inner
		// 	color ctexterror
		&.top:after
			border-top-color rgba(cerror, 1)
		&.bottom:after
			border-bottom-color rgba(cerror, 1)
		&.right:after
			border-right-color rgba(cerror, 1)
		&.left:after
			border-left-color rgba(cerror, 1)

	&.in
		opacity 1
	&.fade
		animation-name fadeIn
		animation-duration .2s
	&.top
		margin-top -7rem
		&:after, &:before
			top 100%
			left 50%
		&:after
			border-top-color #35414C
			margin-left -6rem
		&:before
			border-top-color #D0D3D7
			margin-left -7rem

	&.right
		margin-left 10rem
		&:after, &:before
			right 100%
			top 50%
		&:after
			border-right-color #35414C
			margin-top -6rem
		&:before
			border-right-color #D0D3D7
			margin-top -7rem

	&.bottom
		margin-top 7rem
		&:after, &:before
			bottom 100%
			left 50%
		&:after
			border-bottom-color #35414C
			margin-left -6rem
		&:before
			border-bottom-color #D0D3D7
			margin-left -7rem
	&.left
		margin-left -10rem
		&:after, &:before
			left 100%
			top 50%
		&:after
			border-left-color #35414C
			margin-top -6rem
		&:before
			border-left-color #D0D3D7
			margin-top -7rem


