// Иконки
@font-face
	font-family: "mb-icon-font";
	src:url("../fonts/icons/mb-icon-font.eot");
	src:url("../fonts/icons/mb-icon-font.eot?#iefix") format("embedded-opentype"),
		url("../fonts/icons/mb-icon-font.woff") format("woff"),
		url("../fonts/icons/mb-icon-font.ttf") format("truetype"),
		url("../fonts/icons/mb-icon-font.svg#mb-icon-font") format("svg");
	font-weight: normal;
	font-style: normal;

[data-icon]:before
	font-family: "mb-icon-font" !important;
	content: attr(data-icon);
	font-style: normal !important;
	font-weight: normal !important;
	font-variant: normal !important;
	text-transform: none !important;
	speak: none;
	line-height: 1;
	-webkit-font-smoothing: antialiased;
	-moz-osx-font-smoothing: grayscale;


[class^="icon-"]:before,
[class*=" icon-"]:before
	font-family: "mb-icon-font" !important;
	font-style: normal !important;
	font-weight: normal !important;
	font-variant: normal !important;
	text-transform: none !important;
	speak: none;
	line-height: 1;
	-webkit-font-smoothing: antialiased;
	-moz-osx-font-smoothing: grayscale;


